{
  "name": "@yosemiteyss/pulsefeed-publish",
  "version": "0.2.5",
  "description": "News publish service.",
  "author": "yosemiteyss",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/yosemiteyss/pulsefeed-publish"
  },
  "files": [
    "dist/**"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"{src,apps,libs,test}/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "pretest": "npm run prisma:generate",
    "test": "jest --config jest.json --runInBand",
    "test:watch": "jest --config jest.json --watch",
    "test:cov": "jest --config jest.json --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config jest.json --runInBand",
    "submodule:init": "git submodule init",
    "submodule:update": "git submodule update --remote --recursive",
    "submodule:pull": "cd ./pulsefeed-common && git pull && cd .. && npm install && npm run prisma:generate",
    "prisma:generate": "npx prisma generate --schema=./pulsefeed-common/prisma/schema.prisma"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/microservices": "^10.3.8",
    "@nestjs/platform-express": "^10.3.0",
    "@pulsefeed/common": "file:pulsefeed-common",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "ioredis": "^5.3.2",
    "nest-winston": "^1.9.7",
    "openai": "^4.77.0",
    "pg": "^8.11.5",
    "ramda": "^0.29.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.3.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/ramda": "^0.29.12",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "prettier": "^3.0.0",
    "prettier-plugin-sort-imports": "^1.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}